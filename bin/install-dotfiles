#!/bin/bash

cd ~/Dropbox/Code/General/dotfiles

cp ./gemrc ~/.gemrc
cp ./gitconfig ~/.gitconfig
cp ./gitignore_global ~/.gitignore_global
cp ./tmux.conf ~/.tmux.conf
cp ./vimrc ~/.vimrc
cp ./zshrc ~/.zshrc
cp ./custom/themes/zsh/antonishen.zsh-theme ~/.oh-my-zsh/themes/
cp ./pryrc ~/.pryrc
cp ./ackrc ~/.ackrc
