#!/bin/bash

cd ~/Dropbox/Code/General/dotfiles

cp ./gemrc ~/.gemrc
cp ./gitconfig ~/.gitconfig
cp ./gitignore_global ~/.gitignore_global
cp ./tmux.conf ~/.tmux.conf
cp ./vimrc ~/.vimrc
cp ./zshrc ~/.zshrc
cp ./zprofile ~/.zprofile
mkdir ~/.zsh
cp ./zsh/prompt.zsh ~/.zsh/prompt.zsh
cp ./zsh/dircolors.256dark ~/.zsh/dircolors.256dark
cp ./pryrc ~/.pryrc
cp ./ackrc ~/.ackrc
cp ./colordiffrc ~/.colordiffrc
cp ./xvimrc ~/.xvimrc
cp ./psqlrc ~/.psqlrc
