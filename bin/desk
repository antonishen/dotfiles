#!/bin/bash

tmux start-server
tmux -u new -d -s general
tmux neww -t general
tmux neww -t general

tmux send-keys -t general:1 C-m
tmux send-keys -t general:1 "vim" C-m

tmux send-keys -t general:2 C-m
tmux send-keys -t general:2 "ranger" C-m

tmux rename-window -t general:1 vim
tmux rename-window -t general:2 ranger
tmux rename-window -t general:3 zsh

find . -name "*.taskpaper" -exec open {} +

tmux select-window -t general:1
tmux attach-session -t general
